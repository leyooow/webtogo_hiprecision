/* CSS for admin only */

#OverlayContainer {
	position: absolute;
	width: 100%;
	/*height: 100%;*/
	z-index: 1;
	background: rgba(0, 19, 35, 0.76);
	left: 0px;
	top: 0px;
}

#ModalBox {	
	width: 700px;
	margin-left: auto;
	margin-right: auto;
	min-height: 600px;	
	z-index: 1;
	margin-top: 100px; 
	background-color: #dceeff;
	padding: 5px;
	
}

