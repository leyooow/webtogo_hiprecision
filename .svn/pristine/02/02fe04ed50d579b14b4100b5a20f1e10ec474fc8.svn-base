package com.ivant.cms.dataSource;
import java.util.Date;

import com.ivant.cms.entity.Billing;
public class BillingDataSource {
	private Billing billing;
	private Date billingDate;
	private Date dueDate;
	private String vat;

	private String vatValue;
	private String realValue;
	private String description;
	private String telFax;	

	public BillingDataSource(){
		super();
	}	
	
	public BillingDataSource(Billing billing, Date billingDate,
			 String description, String telFax, String vat) {
		super();
		this.billing = billing;
		this.billingDate = billingDate;
		this.description = description;
		this.telFax = telFax;
		this.vat = vat;
	}

	public Billing getBilling() {
		return billing;
	}

	public void setBilling(Billing billing) {
		this.billing = billing;
	}

	public String getDescription() {
		return description;
	}

	public void setDescription(String description) {
		this.description = description;
	}

	public String getTelFax() {
		return telFax;
	}

	public void setTelFax(String telFax) {
		this.telFax = telFax;
	}

	public void setBillingDate(Date billingDate) {
		this.billingDate = billingDate;
	}

	
	public Date getBillingDate() {
		return billingDate;
	}
	
	public Date getDueDate() {
		return dueDate;
	}

	public void setDueDate(Date dueDate) {
		this.dueDate = dueDate;
	}

	public void setVat(String vat) {
		this.vat = vat;
	}

	public String getVat() {
		return vat;
	}

	public void setVatValue(String vatValue) {
		this.vatValue = vatValue;
	}

	public String getVatValue() {
		return vatValue;
	}

	public void setRealValue(String realValue) {
		this.realValue = realValue;
	}

	public String getRealValue() {
		return realValue;
	}	
}
