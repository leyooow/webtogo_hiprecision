package com.ivant.cms.helper;

import com.ivant.cms.entity.Company;
import com.ivant.cms.entity.FormPage;
import com.ivant.cms.entity.User;

public class FormPageHelper {

	public static FormPage createFormPage(Company company, String name, User createdBy) {
		FormPage formPage= new FormPage();
		formPage.setCompany(company);
		formPage.setName(name);
		formPage.setCreatedBy(createdBy);
		return formPage;
	}
	
}
